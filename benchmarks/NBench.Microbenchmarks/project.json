{
  "version": "0.3.4",
  "title": "NBench",
  "description": "Cross-platform performance benchmarking and testing framework for .NET applications.",
  "authors": [ "Petabridge" ],
  "copyright": "Copyright (c) Petabridge https://petabridge.com/",

  "packOptions": {
    "licenseUrl": "https://github.com/petabridge/NBench/blob/master/LICENSE",
    "projectUrl": "https://github.com/petabridge/NBench",
    "iconUrl": "https://github.com/petabridge/NBench/raw/dev/images/NBench_logo_square_140.png",
    "owners": [ "Petabridge" ],
    "requireLicenseAcceptance": false,
    "tags": [ "performance", "benchmarking", "benchmark", "perf", "testing", "NBench" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/petabridge/NBench"
    }
  },
  "dependencies": {    
    "NBench": { "target": "project" }
  },
  "frameworks": {
    "net452": {
      "buildOptions": {
        "define": [ "THREADPOOL", "APPDOMAIN", "SERIALIZATION" ]
      },
      "dependencies": {
        "BenchmarkDotNet": "0.10.1",
        "BenchmarkDotNet.Core": "0.10.1",
        "BenchmarkDotNet.Toolchains.Roslyn": "0.10.1",
        "Microsoft.CodeAnalysis.Analyzers": "1.1.0",
        "Microsoft.CodeAnalysis.Common": "1.3.2",
        "Microsoft.CodeAnalysis.CSharp": "1.3.2",
        "System.Collections": "4.0.0",
        "System.Collections.Immutable": "1.1.37",
        "System.Diagnostics.Debug": "4.0.0",
        "System.Globalization": "4.0.0",
        "System.Linq": "4.0.0",
        "System.Reflection.Metadata": "1.2.0",
        "System.Resources.ResourceManager": "4.0.0",
        "System.Runtime": "4.0.10",
        "System.Runtime.Extensions": "4.0.0",
        "System.Threading": "4.0.0",
        "System.Threading.Tasks": "4.0.0",
        "System.Threading.Tasks.Extensions": "4.0.0"
      }
    },
    "netcoreapp1.1": {
      "buildOptions": {
        "define": [ "CORECLR" ]
      },
      "dependencies": {
        "BenchmarkDotNet": "0.10.1",
        "BenchmarkDotNet.Core": "0.10.1",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      },
      "imports": [
        "netstandard1.2"
      ]
    }
  }
}
