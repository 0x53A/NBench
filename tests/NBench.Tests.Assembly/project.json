{
  "version": "0.3.4",
  "title": "NBench",
  "description": "Cross-platform performance benchmarking and testing framework for .NET applications.",
  "authors": [ "Petabridge" ],
  "copyright": "Copyright (c) Petabridge https://petabridge.com/",

  "packOptions": {
    "licenseUrl": "https://github.com/petabridge/NBench/blob/master/LICENSE",
    "projectUrl": "https://github.com/petabridge/NBench",
    "iconUrl": "https://github.com/petabridge/NBench/raw/dev/images/NBench_logo_square_140.png",
    "owners": [ "Petabridge" ],
    "requireLicenseAcceptance": false,
    "tags": [ "performance", "benchmarking", "benchmark", "perf", "testing", "NBench" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/petabridge/NBench"
    }
  },
  "dependencies": {    
    "NBench": { "target": "project" }
  },
  "frameworks": {
    "net452": {
      "buildOptions": {
        "define": [ "THREADPOOL", "APPDOMAIN", "SERIALIZATION" ]
      },
      "dependencies": {
        "System.Runtime": "4.3.0",
        "System.Reflection": "4.3.0",
        "Microsoft.Extensions.Configuration": "1.1.0"
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0"
      }
      },
      "netcoreapp1.0": {
        "buildOptions": {
          "define": [ "CORECLR" ]
        },
        "dependencies": {
          "Microsoft.Extensions.Configuration": "1.1.0",
          "CoreCompat.System.Configuration": "4.2.3-r4",
          "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.1.0"
          }
        },
        "imports": [
          "netstandard1.2"
        ]
      }
    }
}
